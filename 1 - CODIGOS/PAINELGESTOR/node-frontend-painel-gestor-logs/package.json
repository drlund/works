{
  "name": "components",
  "version": "0.1.0",
  "description": "Portal de Ferramentas da SuperADM",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "dev": "cross-env HOST=localhost npm run start",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "serve:build": "npx serve -s build -l 3000 # serve na porta 3000 a pasta build",
    "test": "jest --verbose",
    "test:cov": "jest --verbose --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --silent --ci --bail --passWithNoTests",
    "playgrounder": "node test/playgrounder.js",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore --fix . #script de linting base",
    "lint:husky": "eslint --ignore-path .gitignore --ignore-path .eslintignore --fix #script de linting para uso em outros scripts",
    "lint:branchFiles": "lint-prepush #script de lint para os files diferentes da branch master",
    "lint:checkPrettierConflicts": "npx eslint-config-prettier \"{,!(node_modules)/**/}*.{js,jsx}\" #script para checkar se existe conflitos entre eslint e prettier"
  },
