  },
  "browser": {
    "https": "browserify-https"
  },
  "resolutions": {
    "react-scripts/eslint-webpack-plugin": "2.3.0",
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint:husky"
  },
  "lint-prepush": {
    "base": "master",
    "tasks": {
      "*.{js,jsx}": [
        "npm run lint:husky --max-warnings=0"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "defaults",
      "supports es6-module",
      "last 10 versions",
      "Firefox ESR",
      "cover 99.5%",
      "since 2015"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "repository": {
